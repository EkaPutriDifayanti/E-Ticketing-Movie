package detailFilm

import java.io.*;

public class detailFilm implements Serializable{
    private String namaFilm;
    private int tahunRilis;
    private int durasiFilm;
    private String sinopsis;

    public detailFilm(String namaFilm, int tahunRilis, int durasiFilm, String sinopsis ) {
        this.namaFilm = namaFilm;
        this.tahunRilis = tahunRilis;
        this.durasiFilm = durasiFilm;
        this.sinopsis = sinopsis;
    }

    public String getNamaFilm() {
        return namaFilm;
    }

    public void setNamaFilm(String namaFilm) {
        this.namaFilm = namaFilm;
    }

    public int getTahunRilis() {
        return tahunRilis;
    }

    public void setTahunRilis(int tahunRilis) {
        this.tahunRilis = tahunRilis;
    }

    public int getDurasiFilm() {
        return durasiFilm;
    }

    public void setDurasiFilm(int durasiFilm) {
        this.durasiFilm = durasiFilm;
    }

    public String getSinopsis() {
        return sinopsis;
    }

    public void setSinopsis(String sinopsis) {
        this.sinopsis = sinopsis;
    }
}